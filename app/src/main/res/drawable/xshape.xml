<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="10dp"
                    android:color="#0C0C0C"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="10dp"
                    android:color="#101010" />
            </shape>
        </rotate>
    </item>
</layer-list>
